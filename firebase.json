{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
  "headers":[
    {
      "source": "/service-worker.js", "headers":[{"key":"Cache-control", "value":"no-cache"}]
    }
  ]
  }
}
